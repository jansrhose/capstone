// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Admin {
  id                  String          @id @default(cuid()) @map("_id")
  city                String
  barangay            String 
  email               String          @unique
  password            String
}
